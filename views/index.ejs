<script type="text/javascript">
$(document).ready(function () {
	GetLatestReleaseInfo();
});

function GetLatestReleaseInfo() {
	$.getJSON("https://api.github.com/repos/ButchersBoy/doobry/releases/latest").done(function (release) {		
		var asset = findSetupAsset(release.assets);
		var releaseInfo = release.name;
		$(".setup-download-doobry").attr("href", asset.browser_download_url);		
		$(".setup-download-doobry").text("Download " + releaseInfo);		
	});
}

function findSetupAsset(assets) {
	for (var i = 0; i < assets.length; i++) {
		if (assets[i].name.toLowerCase() == "setup.exe")
			return assets[i];	
	}
	return null;
}
</script>
